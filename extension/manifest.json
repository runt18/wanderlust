{
  "name": "Wanderlust",
  "description": "Replace new tab page with a dashboard for vacation inspiration",
  "version": "0.2",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "tabs",
    "http://somewhere.cf/*",
    "https://maps.google.com/maps/api/js?sensor=true",
    "geolocation"
  ],
  "manifest_version": 2
}
